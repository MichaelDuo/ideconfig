{
  "editor.fontFamily": "'JetBrains Mono', 'JetBrains Mono NL', Menlo, Monaco, 'Courier New', monospace",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "workbench.iconTheme": "Monokai Pro (Filter Octagon) Icons",
  "files.autoSave": "afterDelay",
  "editor.formatOnType": true,
  "workbench.list.openMode": "doubleClick",
  "explorer.autoReveal": false,
  "editor.minimap.enabled": false,
  "editor.renderWhitespace": "all",
  "workbench.editor.enablePreview": false,
  "javascript.suggest.autoImports": true,
  "typescript.suggest.autoImports": true,
  "vim.vimrc.enable": false,
  "vim.leader": "\\",
  "vim.easymotion": true,
  "vim.easymotionDimBackground": false,
  "vim.easymotionKeys": "hklyuiopnmqwertzxcvbasdgjf",
  "vim.useSystemClipboard": false,
  "gitlens.menus": {
    "editorGroup": false
  },
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["u"],
      "commands": ["undo"]
    },
    {
      "before": ["<C-r>"],
      "commands": ["redo"]
    },
    {
      "before": ["<leader>", "w"],
      "after": ["<leader>", "<leader>", "<leader>", "b", "d", "w"]
    },
    {
      "before": ["<leader>", "p"],
      "commands": ["workbench.action.pinEditor"],
      "when": "!activeEditorIsPinned"
    },
    {
      "before": ["<leader>", "P"],
      "commands": ["workbench.action.unpinEditor"],
      "when": "activeEditorIsPinned"
    },
    {
      "before": ["<leader>", "C"],
      "commands": ["workbench.action.closeEditorsInGroup"]
    }
  ],
  "workbench.colorTheme": "Monokai Pro (Filter Octagon)",
  "workbench.editor.focusRecentEditorAfterClose": false
}
